{
  "name": "test-stitcher",
  "version": "1.0.0",
  "description": "Extract the test descriptions from a set of tests.",
  "main": "index.js",
  "scripts": {
    "test": "mocha -r esm src/*spec.js",
    "parse-file": "node -r esm ./src/parseFile.js",
    "dev:test": "npm run test -- --watch --bail",
    "typecheck": "tsc",
    "dev:typecheck": "npm run typecheck -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolframkriesing/test-stitcher.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wolframkriesing/test-stitcher/issues"
  },
  "homepage": "https://github.com/wolframkriesing/test-stitcher#readme",
  "devDependencies": {
    "esm": "^3.2.25",
    "mocha": "^7.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
